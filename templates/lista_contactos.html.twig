{% extends "base.html.twig" %}

{% block title %}Contactos
	{% block body %}
		<h1>Listado del contacto</h1>

		{% if contactos %}
			{% for contacto in contactos %}
				{{ include ('partials/datos_contacto.html.twig', {'contacto': contacto}) }}
			{% endfor %}
		{% else %}
			<p>No se han encontrado contactos</p>
		{% endif %}
	{% endblock %}

{% endblock %}
