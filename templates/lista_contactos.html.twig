<!doctype html>

<html>

	<meta charset="utf-8">

	<body>

		<h1>Listado del contacto</h1>

		{% if contactos %}
			{% for contacto in contactos %}
				<ul>
					<li>
						<strong>{{contacto.nombre}}</strong>
					</li>
					<li>
						<strong>{{contacto.telefono}}</strong>
					</li>
					<li>
						<strong>{{contacto.email}}</strong>
					</li>
				</ul>
			{% endfor %}
		{% else %}
			<p>No se han encontrado contactos</p>
		{% endif %}


	</body>

</html>
